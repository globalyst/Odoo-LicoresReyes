<openerp>
    <data>
		<!-- VISTA DE ORDENES DE VENTA, PESTANAS DE OFERTAS -->
		<record id="product_form_view_licores" model="ir.ui.view">
			<field name="name">product.template.common.form.licores</field>
			<field name="model">product.template</field>
			<field name="inherit_id" ref="product.product_template_form_view"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='currency_id']" position="after">
					<field name="graduacion" string="Graduacion Alcoholica"/>
					<field name="format_id" string="Formato"/>
				</xpath>
			</field>
		</record>
	</data>
</openerp>